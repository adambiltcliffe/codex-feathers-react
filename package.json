{
  "name": "codex-feathers-react",
  "description": "Game server and browser client for Codex",
  "version": "0.1.0",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Adam Biltcliffe",
    "email": "adam.biltcliffe@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run eslint && npm run jest",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "nodemon src/",
    "start": "node src/",
    "clean": "shx rm -rf test/data/",
    "jest": "npm run clean && cross-env NODE_ENV=test jest --runInBand",
    "babel": "shx rm -rf codex && babel node_modules/@adam.biltcliffe/codex/src --out-dir codex --ignore \"src/**/*.test.js\" --plugins @babel/plugin-transform-modules-commonjs"
  },
  "dependencies": {
    "@adam.biltcliffe/codex": "^0.1.0",
    "@feathersjs/authentication": "^4.3.0",
    "@feathersjs/authentication-local": "^4.3.0",
    "@feathersjs/authentication-oauth": "^4.3.0",
    "@feathersjs/configuration": "^4.3.0",
    "@feathersjs/errors": "^4.3.0",
    "@feathersjs/express": "^4.3.0",
    "@feathersjs/feathers": "^4.3.0",
    "@feathersjs/socketio": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "es6-dynamic-template": "^2.0.0",
    "feathers-authentication-hooks": "^0.3.1",
    "feathers-hooks-common": "^4.20.7",
    "feathers-nedb": "^5.0.1",
    "helmet": "^3.20.1",
    "nedb": "^1.8.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "axios": "^0.19.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rollup": "^1.21.2",
    "shx": "^0.3.2"
  }
}
